<nav id="mainNavigation">
  <div class="logo"><img src="/assets/images/logo-symbol.png" alt=""></div>

  <ul>
    <% menu.forEach(function(item) { %>
      <li>
        <!-- current.source -->
        <a href="#0" <% if(menuActive == item.data) { %>class="selected"<% } %> data-menu="<%= item.data %>">
          <%- item.icon %>
          <%= item.title %>
        </a>
      </li>
    <% }); %>
  </ul>

  <div class="social">
    <% social.forEach(function(item) { %>
      <a href="<%= item.link %>" target="_blank">
        <%- item.icon %>
      </a>
    <% }); %>
  </div>
</nav>
